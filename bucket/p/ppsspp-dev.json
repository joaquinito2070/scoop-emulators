        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-964-ge7f1716c4&platform=windows-amd64#/dl.7z",
            "hash": "81fa5dccd5aaae935043404437c8628af710f61ef4beaa184591e9cad1fb9731",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
