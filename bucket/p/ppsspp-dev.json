        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-723-gf6573a4d7&platform=windows-amd64#/dl.7z",
            "hash": "5c591aec7bcb53473cb4f5052bd298446056939d555e28f5feef3d849a515d65",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
