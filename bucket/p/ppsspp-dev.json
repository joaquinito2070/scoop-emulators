        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.15.4-202-g0fb7d6f7d&platform=windows-amd64#/dl.7z",
            "hash": "692fb6c7456af70cf19929b235080f968cca525567d99463267ddd0581bbedf1",
