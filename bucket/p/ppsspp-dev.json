        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-1218-gecfd4759d&platform=windows-amd64#/dl.7z",
            "hash": "7184d5a3e3896e5ac676d96d9ed78c883b614d059670d1561731c109c46268b3",
