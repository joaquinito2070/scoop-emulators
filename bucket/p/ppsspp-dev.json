            "shortcuts": [
                [
                    "PPSSPPWindows.exe",
                    "PPSSPP (dev)"
                ]
            ]
        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-1147-g3c9e968ca&platform=windows-amd64#/dl.7z",
            "hash": "1eee742d598838713888610f2662e16273afced4d9d0d85732f8859c30bc7cd6",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
                ]
            ],
