        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-1030-g2f09d2969&platform=windows-amd64#/dl.7z",
            "hash": "d944725617e8e63068b8937e9219af0495d5dd23cbbb96b2c320a64820764a62",
