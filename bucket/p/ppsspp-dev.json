        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-933-g5b0748a61&platform=windows-amd64#/dl.7z",
            "hash": "c02a139d39d4f7222ec952a8ee8027846fa0306584006f44f6b77590afea13db",
            "bin": [
