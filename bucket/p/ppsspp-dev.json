        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-1228-g5c1e66a91&platform=windows-amd64#/dl.7z",
            "hash": "49d9aa1ec02699e979444fc709173b85ba78cc43fc79093199d0b23799086710",
