        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-1150-gd65afcc7f&platform=windows-amd64#/dl.7z",
            "hash": "e2a128070830082a9ff78ea58d39dcd4eec85c9270935aaae3c5b8d8d07e3fdd",
            "bin": [
