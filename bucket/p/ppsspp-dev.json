        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.15.4-94-g69b5b3641&platform=windows-amd64#/dl.7z",
            "hash": "03166db4858008c8d39810e5413856581744b6fd3e70d7c9a903d794c0d29302",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
