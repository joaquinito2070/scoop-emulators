        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.15.4-75-g6f380a7a0&platform=windows-amd64#/dl.7z",
            "hash": "eb7a22c48232fa7fe067c215e3712db3a3963ddc4ecad9a3050181bb8b1774fd",
