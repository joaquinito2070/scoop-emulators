        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-834-g3feaa78c1&platform=windows-amd64#/dl.7z",
            "hash": "1aa938976aacbe4cd99d6318916a9e71eb698f53ab3ada0d68496db57a5fb27b",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
