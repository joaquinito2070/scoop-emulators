        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-704-ge9182a6e0&platform=windows-amd64#/dl.7z",
            "hash": "6b7a9225ce61162669a8682cc32282624de23bf760ae0987905aa04e6728f58f",
